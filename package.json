  "scripts": {
    "changelog": "changelog",
    "tsc": "tsc",
    "typings": "typings",
    "typedoc": "typedoc",
    "webpack": "webpack",
    "tslint": "tslint",
    "npm-check-updates": "npm-check-updates -a",
    "ncu": "ncu -a",
    "webpack-dev-server": "webpack-dev-server",
    "build": "npm run build:dev",
    "build:dev": "concurrently \"webpack --config ./config/webpack/webpack.dev.babel.js --progress --profile --colors --display-error-details --display-cached\" && tsc",
    "build:prod": "concurrently \"webpack --config ./config/webpack/webpack.prod.babel.js  --progress --profile --colors --display-error-details --display-cached --bail\" \"tsc\"",
    "server": "npm run server:dev",
    "server:dev": "DASHBOARD=true webpack-dashboard -- webpack-dev-server --config config/webpack/webpack.dev.babel.js --progress --profile --watch --content-base demo/",
    "server:prod": "DASHBOARD=true webpack-dashboard -- npm run build:prod && http-server build --cors",
    "start": "npm run server:dev",
    "start:prod": "npm run server:prod",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "test": "karma start",
    "tests": "npm run test",
    "test:watch": "npm run watch:test",
    "codeclimate": "codeclimate-test-reporter < build/reports/lcov-report/lcov.info",
    "docs": "npm run typedoc -- --options ./typedoc.json --tsconfig ./config/typedoc/tsconfig.json --exclude '**/*.spec.ts' ./components/"
  }
